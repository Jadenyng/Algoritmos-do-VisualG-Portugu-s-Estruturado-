algoritmo "Exercício 5"

// Função : Pedir duas notas e exibir sua média
// Autor : Pedro Thalysson Balbino Araújo
// Data : 11/07/2022

var
N1:real
N2:real
MED:real

inicio

ESCREVA("Primeira nota: ")
LEIA(N1)

ESCREVA("Segunda nota: ")
LEIA(N2)

MED <- (N1+N2)/2

ESCREVA("A média entre",N1," e",N2," é",MED,"")

fimalgoritmo
